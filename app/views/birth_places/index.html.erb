<p id="notice"><%= notice %></p>

<h1>Listing Birth Places</h1>

<table>
  <thead>
    <tr>
      <th>Birth</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @birth_places.each do |birth_place| %>
      <tr>
        <td><%= birth_place.birth %></td>
        <td><%= link_to 'Show', birth_place %></td>
        <td><%= link_to 'Edit', edit_birth_place_path(birth_place) %></td>
        <td><%= link_to 'Destroy', birth_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Birth place', new_birth_place_path %>
